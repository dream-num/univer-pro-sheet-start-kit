{
  "name": "univer-pro-sheet-start-kit",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "A starter kit for Univer Server",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "build:pkg": "pnpm build && cd .github/server && pnpm build:pkg",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@univerjs/action-recorder": "0.10.14",
    "@univerjs/presets": "0.10.14",
    "@univerjs/sheets-crosshair-highlight": "0.10.14",
    "@univerjs/sheets-zen-editor": "0.10.14",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.1.0",
    "@types/node": "^24.9.2",
    "eslint": "^9.38.0",
    "eslint-plugin-format": "^1.0.2",
    "lint-staged": "^16.2.6",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
