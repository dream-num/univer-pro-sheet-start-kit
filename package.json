{
  "name": "univer-pro-sheet-start-kit",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "A starter kit for Univer Server",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "build:pkg": "pnpm build && cd .github/server && pnpm build:pkg",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@univerjs/action-recorder": "0.9.3",
    "@univerjs/presets": "0.9.3",
    "@univerjs/sheets-crosshair-highlight": "0.9.3",
    "@univerjs/sheets-zen-editor": "0.9.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "eslint": "^9.28.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^16.1.0",
    "simple-git-hooks": "^2.13.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
